{
  "name": "timsteinhauer/livewire-crud",
  "description": "Livewire CRUD scaffolding for the Laravel framework.",
  "keywords": ["laravel", "livewire", "crud"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/timsteinhauer/livewire-crud/issues",
    "source": "https://github.com/timsteinhauer/livewire-crud"
  },
  "authors": [
    {
      "name": "Tim Steinhauer",
      "email": "t.steinhauer@high-office.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "laravel/framework": "^8.0|^9.0",
    "livewire/livewire": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Timsteinhauer\\LivewireCrud\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Timsteinhauer\\LivewireCrud\\JetstreamServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
